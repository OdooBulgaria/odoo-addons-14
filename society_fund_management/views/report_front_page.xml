<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <template id="society_fund_management.report_front_page_member">
      <t t-foreach="getPartner(data)" t-as="l">
        <div class="page">
          <table class="table table-condensed ">
            <tbody>
              <tr style="font-size:30;border-top-style:hidden;">
                <td>Account No. :
                </td>
                <td><span t-esc="l['account_no']"/></td>
              </tr>
              <tr style="font-size:25;border-top-style:hidden;">
                <td>Name :</td>
                <td><span t-esc="l['display_name']"/></td>
              </tr>
              <tr style="font-size:25;border-top-style:hidden;">
                <td>Village :</td>
                <td><span t-esc="l['phone']"/></td>
              </tr>
              <tr style="font-size:25;border-top-style:hidden;">
                <td>Mobile No. :</td>
                <td><span t-esc="l['mobile']"/></td>
              </tr>
              <tr style="font-size:25;border-top-style:hidden;">
                <td>Address :</td>
                <td><span t-esc="l['street']"/></td>
              </tr>
              <tr style="font-size:25;border-top-style:hidden;">
                <td/>
                <td><span t-esc="l['street2']"/></td>
              </tr>
              <tr style="font-size:25;border-top-style:hidden;">
                <td/>
                <td><span t-esc="l['city']"/></td>
              </tr>
              <tr style="font-size:25;border-top-style:hidden;">
                <td/>
                <td><span t-esc="l['zip']"/></td>
              </tr>

            </tbody>
          </table>
        </div>
      </t>
    </template>

    <report id="report_front_page" string="Payment Front Page" model="res.partner" menu="False" report_type="qweb-pdf" file="society_fund_management.report_front_page_member" name="society_fund_management.report_front_page_member"/>

    <record id="society_fund_management.report_front_page" model="ir.actions.report.xml">
      <field name="paperformat_id" ref="society_fund_management.paper_format_front_page"/>
    </record>

  </data>
</openerp>
