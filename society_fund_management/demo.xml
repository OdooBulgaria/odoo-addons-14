<openerp>
    <data>
      <!-- Account Journals -->
      <record id="account_loan_journal" model="account.journal">
          <field name="code">LN</field>
          <field name="name">Loan Journal</field>
          <field name="type">sale</field>
      </record>
      <record id="account_scheme_journal" model="account.journal">
          <field name="code">SCM</field>
          <field name="name">scheme Journal</field>
          <field name="type">purchase</field>
      </record>
      <record id="account_ps_journal" model="account.journal">
          <field name="code">APS</field>
          <field name="name">Profit Sharing Journal</field>
          <field name="type">purchase</field>
      </record>


      <!-- Accounts -->
      <record id="account_loan_account" model="account.account">
          <field name="name">Loan Account</field>
          <field name="code">LNACC</field>
          <field name="type">other</field>
          <field name="user_type" ref="account.data_account_type_income"/>
          <field name="reconcile" eval="False"/>
      </record>
      <record id="account_scheme_account" model="account.account">
          <field name="name">scheme Account</field>
          <field name="code">SCACC</field>
          <field name="type">other</field>
          <field name="user_type" ref="account.data_account_type_expense"/>
          <field name="reconcile" eval="False"/>
      </record>
      <record id="account_panalty_account" model="account.account">
          <field name="name">Panalty and Interest Account</field>
          <field name="code">PN/INTACC</field>
          <field name="type">other</field>
          <field name="user_type" ref="account.data_account_type_income"/>
          <field name="reconcile" eval="False"/>
      </record>

      <!-- Analytic Account -->
      <record id="analytic_fund_costing_root" model="account.analytic.account">
          <field name="name" model="res.company" use="name" search="[]"/>
      </record>
      <record id="analytic_fund_costing_internal" model="account.analytic.account">
          <field name="name">Interest/Panalty Costing</field>
          <field name="type">view</field>
          <field name="parent_id" ref="society_fund_management.analytic_fund_costing_root"/>
      </record>
      <record id="analytic_interest_society" model="account.analytic.account">
          <field name="name">Interest Account</field>
          <field name="type">normal</field>
          <field name="parent_id" ref="society_fund_management.analytic_fund_costing_internal"/>
      </record>
      <record id="analytic_panalty_society" model="account.analytic.account">
          <field name="name">Panalty Account</field>
          <field name="type">normal</field>
          <field name="parent_id" ref="society_fund_management.analytic_fund_costing_internal"/>
      </record>

      <!-- Analytic Journal-->
      <record id="fund_analytic_journal_income" model="account.analytic.journal">
          <field name="code">INC</field>
          <field name="name">Total Income</field>
          <field name="type">sale</field>
      </record>

    </data>
</openerp>
